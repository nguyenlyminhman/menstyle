<div class="col-md-3 single-right">
    <div class="w_sidebar">
        <section class="sky-form">
            <h4>Catogories</h4>
            <div class="row1 row2 scroll-pane">
                 <div class="col col-4">
                    <label class="radio"><input type="radio" name="categogies" checked=""><i></i>All Accessories</label>
                </div> 
                <div class="col col-4">
                    <%mainMenu.forEach((data)=>{%>
                        <label class="radio"><input type="radio" name="categogies"><i></i><%=data.cate_name%></label>
                        <%})%>
                </div>
            </div>
        </section>
        <section class="sky-form">
            <h4>Brand</h4>
            <div class="row1 row2 scroll-pane">
                 <div class="col col-4">
                    <label class="radio"><input type="radio" name="products" checked=""><i></i>All Products</label>
                </div> 
                <div class="col col-4">
                    <% brandMenu.forEach((data)=>{%>
                        <label class="radio"><input type="radio" name="products"><i></i><%=data.producer_name%></label>
                        <%})%>
                </div>
            </div>
        </section>
        <section class="sky-form">
            <h4>size</h4>
            <div class="row1 row2 scroll-pane">
                <div class="col col-4">
                    <label class="radio"><input type="radio" name="radio"><i></i>5.5 - 6.5</label>
                    <label class="radio"><input type="radio" name="radio"><i></i>7.0 - 8.0 </label>
                    <label class="radio"><input type="radio" name="radio"><i></i>8.5 - 9.5</label>
                    <label class="radio"><input type="radio" name="radio"><i></i>10.0 - 11.0</label>
                    <label class="radio"><input type="radio" name="radio"><i></i>11.5 - 12.5</label>
                    <label class="radio"><input type="radio" name="radio"><i></i>13.5 - 15.5</label>
                </div>
            </div>
        </section>
